#!/bin/bash
mkdir laba1 #создание папки в которую помещается репозиторий
cd laba1
echo "Вставьте ссылку репозитория"
read temp #считывание ссылки
git clone $temp
cd ~
mkdir laba2
mv ../laba1 ~/laba2 #перенос файлов с одной папки в другую
rm -R ~/laba1 #удаление первой папки
chmod -R 500 laba2 #предоставление доступа 
echo "Лабораторная работа  №1 выполнена"
